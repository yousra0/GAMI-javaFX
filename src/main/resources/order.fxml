<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.OrderController">
    <TilePane fx:id="orderTilePane" prefWidth="600" prefHeight="400" hgap="10" vgap="10"/>

    <ListView fx:id="orderList" prefWidth="200" prefHeight="200"/>

    <Button fx:id="deleteSelectedOrdersButton" text="Delete Selected Orders" onAction="#deleteOrder"/>

    <Label fx:id="messageLabel" />
</VBox>
